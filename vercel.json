  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "file:./prisma/dev.db"
  },
  "build": {
    "env": {
      "DATABASE_URL": "file:./prisma/dev.db"
    }
  }
}